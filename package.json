{
  "name": "mongoose-model-decorators",
  "version": "0.2.4",
  "description": "ES2016 decorator functions for building Mongoose models.",
  "license": "MIT",
  "repository": "goto-bus-stop/mongoose-model-decorators",
  "author": "Ren√© Kooi <rene@kooi.me>",
  "scripts": {
    "build": "babel --out-dir . src",
    "prepublish": "npm run build",
    "test:lint": "standard test.js src/**/*.js",
    "test:compile": "babel test.js > test.compiled.js",
    "test:clean": "rm test.compiled.js",
    "test": "npm run test:compile && ava test.compiled.js && npm run test:clean && npm run test:lint"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "files": [
    "index.js",
    "Model.js",
    "Plugin.js",
    "Schema.js"
  ],
  "keywords": [
    "mongoose",
    "decorator",
    "decorators",
    "model"
  ],
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "mongoose": "4.x",
    "standard": "^6.0.4"
  },
  "peerDependencies": {
    "mongoose": "4.x"
  }
}
